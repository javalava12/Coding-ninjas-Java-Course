import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {

		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int i = 1;
		while (i <= n) {
			int col = 1;
			int value = i;
			while (col <= i) {
				System.out.print(value);
				value = value + 1;
				col = col + 1;
			}
			System.out.println();
			i = i + 1;
		}

	}
}
